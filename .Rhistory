get_size = function(diagnoal, horizontal, vertical) {
m = sqrt(diagnoal^2 / (horizontal^2 + vertical^2))
c(width = horizontal * m, height = vertical * m)
}
d = c(49, 43, 34, 32, 27)
h = c(32, 16, 21, 16, 16)
v = c(9, 9, 9, 9, 9)
sizetbl = pmap_dfr(list(d, h, v), ~{
